<div>

    <table class="table table-striped">
        <tr style="font:bold;font-size:large;">
            <td>Product Id</td>
            <td>Name</td>
            <td>Description</td>
        </tr>
        @foreach (var product in ViewBag.ProductCollection)
        {
            <tr class="paginate" >
                <td>@product.ProductId</td>
                <td>@product.Name</td>
                <td>@product.Description</td>
            </tr>
        }
    </table>
    <br>
    <div id="page-nav"></div>


</div>
